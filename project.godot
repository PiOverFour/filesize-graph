; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Filesize Graph"
config/version="0.4.0"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.4")
boot_splash/fullsize=false

[debug]

settings/crash_handler/message=""

[display]

window/energy_saving/keep_screen_on=false
window/subwindows/embed_subwindows=false
window/per_pixel_transparency/allowed=true
window/size/width=960
window/size/height=540

[internationalization]

locale/translations=PackedStringArray("res://translations/text.fr.po")
locale/translations_pot_files=PackedStringArray("res://Main.tscn", "res://EditMenu.gd", "res://FileMenu.gd", "res://ImageSequence.gd", "res://Main.gd")

[rendering]

renderer/rendering_method="gl_compatibility"
vram_compression/import_s3tc=false
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
